<h1 id="page_title">Stuff I might write</h1>

      <div id="posts_wrapper" class="skinny_wrapper">
        <div class="posts">
          <% @posts.each do |post| %>
          <p class="date"><%= post.created_at.strftime('%A %b %d')%> %></p>
          <h2><a href="#"><%= link_to post.title, post %></a></h2>
          <hr>
          <% end %>

      <div class="pagination">
        <span class="previous_page disabled">
          Previous
        </span>
        <em class="current">1</em>
        <a rel='next' href="#">2</a>
        <a href="#" class="next_page">Next</a>
      </div>
